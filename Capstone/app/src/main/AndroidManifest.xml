<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cap.withsullivan" >
    <!-- 인터넷 권한 -->
    <uses-permission android:name="android.permission.INTERNET" /> <!-- GPS 권한 -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" /> <!-- 카메라 권한 -->
    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:name=".GlobalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Withsullivan"
        android:usesCleartextTraffic="true" >
        <activity
            android:name=".DetectActivity"
            android:exported="true" />
        <!-- 카카오맵 네이티브 App Key -->
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="e8f3732d36d621e7d5afb540cd9fd88c" />

        <activity
            android:name=".MapActivity"
            android:exported="true" />
        <activity
            android:name=".MenuActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- 카카오로그인 네이티브 App Key -->
        <activity
            android:name="com.kakao.sdk.auth.AuthCodeHandlerActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="oauth"
                    android:scheme="kakaoe8f3732d36d621e7d5afb540cd9fd88c" />
            </intent-filter>
        </activity>
    </application>

</manifest>